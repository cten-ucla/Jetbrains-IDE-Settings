<toolSet name="External Tools">
  <tool name="Maple" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/Library/Frameworks/Maple.framework/Versions/2020/bin/maple" />
      <option name="PARAMETERS" value="-q $FilePath$" />
      <option name="WORKING_DIRECTORY" value="$FileParentDir$" />
    </exec>
  </tool>
  <tool name="MacSpice" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="true" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/Applications/MacSpice.app/Contents/MacOS/MacSpice" />
      <option name="PARAMETERS" value="-b $FilePath$ -o &quot;$FileNameWithoutAllExtensions$.txt&quot;" />
      <option name="WORKING_DIRECTORY" value="$FileDir$" />
    </exec>
  </tool>
  <tool name="LTSpice" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="true" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/Applications/LTspice.app/Contents/MacOS/LTspice" />
      <option name="PARAMETERS" value="-b $FilePath$" />
      <option name="WORKING_DIRECTORY" value="$FileDir$" />
    </exec>
  </tool>
  <tool name="Black" description="Black is the uncompromising Python code formatter." showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="false" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="$ContentRoot$/venv/bin/black" />
      <option name="PARAMETERS" value="&quot;$FilePath$&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  <tool name="Matlab" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="/Applications/MATLAB_R2020b.app/bin/matlab" />
      <option name="PARAMETERS" value="-nodesktop -nosplash $FileDirName$" />
      <option name="WORKING_DIRECTORY" value="$FileParentDir$" />
    </exec>
  </tool>
</toolSet>